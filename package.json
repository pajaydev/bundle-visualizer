{
  "name": "ebay-bundle-visualizer",
  "version": "1.0.0",
  "author": "Ajaykumar prathap",
  "license": "MIT",
  "description": "Chrome extension to visualize the ebay website",
  "dependencies": {
    "rollup-plugin-uglify": "^6.0.4",
    "uglify-es": "^3.3.9"
  },
  "scripts": {
    "minify-popup": "uglifyjs src/popup.js -o dist/popup.min.js",
    "minify-webtree": "uglifyjs src/webtreemap.js -o dist/webtreemap.min.js",
    "compile": "npm run minify-popup && npm run minify-webtree"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:pajaydev/bundle-visualizer.git"
  },
  "keywords": [
    "ebay",
    "bundle",
